{
  "name": "@phixid/in-memory-store",
  "version": "1.0.4",
  "description": "An in-memory data-store intended to be used as a simple cache",
  "author": "Kristof Hermans <kristof.hermans.kh@gmail.com>",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "build": "yarn build:cjs & yarn build:es",
    "build:cjs": "NODE_ENV=CJS rollup -c",
    "build:es": "NODE_ENV=ES rollup -c",
    "clean": "rm -rf node_modules lib",
    "declare": "cp lib/index.d.ts lib/index.cjs.d.ts && mv lib/index.d.ts lib/index.es.d.ts",
    "test": "jest --forceExit",
    "prebuild": "yarn clean",
    "postbuild": "rm -rf '.rpt2_cache' lib/*.spec.d.ts lib/test-utils.d.ts && yarn declare",
    "postclean": "yarn"
  },
  "repository": {
    "url": "git+https://github.com/phixid/in-memory-store.git",
    "type": "GIT"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/node": "7.2.2",
    "@babel/plugin-transform-runtime": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.4.0",
    "@types/jest": "24.0.11",
    "babel-jest": "24.5.0",
    "jest": "24.5.0",
    "rollup": "1.7.0",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-typescript2": "0.20.1",
    "typescript": "3.3.4000"
  },
  "bugs": {
    "url": "https://github.com/phixid/in-memory-store/issues"
  },
  "homepage": "https://github.com/phixid/in-memory-store#readme"
}
